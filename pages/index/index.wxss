/* pages/index/index.wxss */
.container{
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    padding: 20rpx;
}

.container .list{
    display: flex;
    flex-direction: column;
    align-items: center;
    height: max-content;
}

.container .list .box{
  width: 130rpx;
  height: 130rpx;
  background-color: #fff;
  border-radius: 24rpx;
  text-align: center;
}
.container .list .box .image_logos{
  width: 130rpx;
  height: 130rpx;
  border-radius: 24rpx;
}
.container .list .text{
    font-family: PingFang SC;
    font-weight: 400;
    font-size: 26rpx;
    height: 17px;
    line-height: 17px;
    text-align: center;
    margin: 20rpx 0;
}