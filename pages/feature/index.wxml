    <button class="save public" bindtap="onSaveImage">‰øùÂ≠òÂõæÁâá</button>
    <!-- open-type="share" Âè™Âú® button ÁªÑ‰ª∂‰∏äÁîüÊïà„ÄÇ -->
    <button class="share public" open-type="share">ÂàÜ‰∫´</button>
  </view>

  <wxml2canvas id="wxml2canvas" />
</view>